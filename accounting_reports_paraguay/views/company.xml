<odoo>
    <data>
        <record id="py_account_company_form_inh_repartos" model="ir.ui.view">
            <field name="name">account.paraguay.report.company</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="paraguay_backoffice.base_company_form_inherited"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[2]" position="inside">
	                <group string="Datos Res. 49">
                        <group>
                            <field name="contador" />
                            <field name="ruc_contador"/>
                            <field name="auditor"/>
                            <field name="ruc_auditor"/>
                        </group>
                    </group>
                </xpath>
                <xpath expr="//notebook/page[2]" position="after">
	                <page string="Anexo 3">
                        <group>
                            <field name="contador" />
                            <field name="ruc_contador"/>
                            <field name="auditor"/>
                            <field name="ruc_auditor"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>