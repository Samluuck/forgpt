<odoo>
    <data>
        <!--        Se hereda la vista del Diario-->
        <record id="view_account_account_inh" model="ir.ui.view">
            <field name="name">view.account.account.paraguay.inh</field>
            <field name="model">account.account</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="account.view_account_form"/>
            <field name="arch" type="xml">
            <xpath expr="//field[@name='group_id']" position="after">
                     <field name="cod_eerr"/>
            </xpath>
            </field>
        </record>
        <record id="search_account_account_inh" model="ir.ui.view">
            <field name="name">search_account_account_inh</field>
            <field name="model">account.account</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="account.view_account_search"/>
            <field name="arch" type="xml">
            <xpath expr="//field[@name='user_type_id']" position="after">
                     <field name="cod_eerr"/>
            </xpath>
            </field>
        </record>
    </data>
</odoo>