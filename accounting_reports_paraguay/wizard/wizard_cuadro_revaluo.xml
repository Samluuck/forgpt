<odoo>
    <data>
        <record id="reporte_cuadro_revaluo" model="ir.ui.view">
            <field name="name">wizard.cuadro.revaluo</field>
            <field name="model">cuadro.revaluo.wizard</field>
            <field name="arch" type="xml">
               <form>
                  <group>
                      <field name="periodo"/>
                      <field name="fecha_inicio" invisible="1"/>
                      <field name="fecha_fin" invisible="1"/>
                      <field name="company_id" invisible="1"/>

                  </group>


                   <footer>
                       <button name="check_report" type="object" string="Generar PDF" class="oe_highlight"/>
                       <!--<button name="print_report_xlsx" type="object" string="Generar Xlsx" class="oe_highlight"/>-->
                       <button string="Cancelar" special="cancel"/>
                   </footer>
               </form>
            </field>
        </record>

        <record id="action_reporte_cuadro_revaluo" model="ir.actions.act_window">
            <field name="name">Cuadro de revaluo </field>
            <field name="res_model">cuadro.revaluo.wizard</field>
<!--            <field name="type">ir.actions.act_window</field>-->
<!--            <field name="view_type">form</field>-->
<!--            <field name="view_mode">form</field>-->
            <field name="view_id" ref="reporte_cuadro_revaluo"/>
            <field name="target">new</field>
        </record>



    </data>
</odoo>