<odoo>
    <data>
        <record id="reporte_estado_patrimonio" model="ir.ui.view">
            <field name="name">estado_patrimonio.wizard.estado_patrimonio</field>
            <field name="model">estado_patrimonio.wizard.estado_patrimonio</field>
            <field name="arch" type="xml">
               <form>
                  <group>

                      <field name="fecha_inicio" required="True"/>
                      <field name="fecha_fin" required="True"/>
                      <field name="company_id" invisible="1"/>

                  </group>


                   <footer>
                       <button name="check_report" type="object" string="Generar PDF" class="oe_highlight"/>
                       <button name="print_report_xlsx" type="object" string="Generar Xlsx" class="oe_highlight"/>
                       <button string="Cancelar" special="cancel"/>
                   </footer>
               </form>
            </field>
        </record>

        <record id="action_reporte_estado_patrimonio" model="ir.actions.act_window">
            <field name="name">Estado de Patrimonio </field>
            <field name="res_model">estado_patrimonio.wizard.estado_patrimonio</field>
<!--            <field name="type">ir.actions.act_window</field>-->
<!--            <field name="view_type">form</field>-->
            <field name="view_mode">form</field>
            <field name="view_id" ref="reporte_estado_patrimonio"/>
            <field name="target">new</field>
        </record>



    </data>
</odoo>
