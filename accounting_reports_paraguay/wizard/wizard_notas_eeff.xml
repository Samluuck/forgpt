<odoo>
    <data>
        <record id="reporte_notas_eeff" model="ir.ui.view">
            <field name="name">notas.wizard.notas.eeff</field>
            <field name="model">notas.wizard.notas.eeff</field>
            <field name="arch" type="xml">
               <form>
                  <group>

                      <field name="fecha_inicio" required="True"/>
                      <field name="fecha_fin" required="True"/>
                      <field name="company_id" invisible="1"/>
                  </group>



                   <footer>
                       <button name="check_report" type="object" string="Generar PDF" class="oe_highlight"/>
                       <!--<button name="print_report_xlsx" type="object" string="Generar Xlsx" class="oe_highlight"/>-->
                       <button string="Cancelar" special="cancel"/>
                   </footer>
               </form>
            </field>
        </record>

        <record id="action_reporte_notas_eff" model="ir.actions.act_window">
            <field name="name">Notas a los Estados Financieros </field>
            <field name="res_model">notas.wizard.notas.eeff</field>
<!--            <field name="type">ir.actions.act_window</field>-->
<!--            <field name="view_type">form</field>-->
<!--            <field name="view_mode">form</field>-->
            <field name="view_id" ref="reporte_notas_eeff"/>
            <field name="target">new</field>
        </record>




    </data>
</odoo>