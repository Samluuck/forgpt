<odoo>
  <record id="action_import_wizard_from_tree" model="ir.actions.server">
    <field name="name">Importar asistencias .dat</field>
    <field name="model_id" ref="hr_attendance.model_hr_attendance"/>
    <field name="binding_model_id" ref="hr_attendance.model_hr_attendance"/>
    <field name="state">code</field>
    <field name="binding_view_types">list</field>
    <field name="groups_id" eval="[(4, ref('base.group_user'))]"/>
    <field name="code">
      action = env.ref('attendance_dat_import.attendance_import_wizard_action_window').read()[0]
    </field>
  </record>
</odoo>