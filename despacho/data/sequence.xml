<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="increment_ot_sequence" model="ir.sequence" noupdate="1">
        <field name="name">Despachos</field>
        <field name="code">despacho.despacho.sequence</field>
        <field name="prefix">OT</field>
        <field name="padding">5</field>
        <field name="number_next">3000</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="increment_op_sequence" model="ir.sequence" noupdate="1">
        <field name="name">OrdenesPago</field>
        <field name="code">despacho.ordenpago.sequence</field>
        <field name="prefix">OP</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="increment_p_sequence" model="ir.sequence" noupdate="1">
        <field name="name">Pagos</field>
        <field name="code">despacho.pago.sequence</field>
        <field name="prefix">P</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>