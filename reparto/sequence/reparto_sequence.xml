<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0"      >

        <record id="numero_reparto_sequence" model="ir.sequence">
            <field name="name">Reparto</field>
            <field name="prefix">REP/%(year)s/</field>
            <field name="padding">5</field>
            <field name="code">reparto.sequence</field>
            <field name="number_increment">1</field>
            <field eval="True" name="active"/>
            <field name="implementation">no_gap</field>
        </record>
        
        <record id="numero_reparto_padre_sequence" model="ir.sequence">
            <field name="name">Reparto Padre</field>
            <field name="prefix">REPARTO/%(year)s/</field>
            <field name="padding">5</field>
            <field name="code">reparto.padre.sequence</field>
            <field name="number_increment">1</field>
            <field eval="True" name="active"/>
            <field name="implementation">no_gap</field>
        </record>

    </data>
</odoo>
